  "code": "export default class RoleBullet extends Laya.Script {\r\n    constructor() {\r\n        super();\r\n        this.bulletType = 1;\r\n        this.direction = 1;\r\n        this.speed = 35;\r\n        this.dis = 0;\r\n    }\r\n    onAwake() {\r\n        this.view = fairygui.UIPackage.createObject(\"Game\", \"zidan\");\r\n        this.zidan = this.view.getChildAt(0).asLoader;\r\n        Laya.Scene.load(\"RoleBullet.scene\", Laya.Handler.create(this, (s) => {\r\n            this.scene = s;\r\n            this.scene.addChild(this.view.displayObject);\r\n            this.body = this.scene.getComponent(Laya.RigidBody);\r\n            this.zidan.url = \"ui://Game/zidan\" + this.bulletType;\r\n            if (this.direction == 1) {\r\n                this.view.setSkew(0, 0);\r\n                this.body.setVelocity({ x: 15, y: 0 });\r\n            }\r\n            else {\r\n                this.view.setSkew(180, 180);\r\n                this.body.setVelocity({ x: -15, y: 0 });\r\n            }\r\n        }));\r\n    }\r\n    setBulletData(type, dir) {\r\n        this.bulletType = type;\r\n        this.direction = dir;\r\n    }\r\n    onUpdate() {\r\n        console.log(\"bulletFly\");\r\n        this.dis += this.speed;\r\n        if (this.direction == 1) {\r\n            this.view.x += this.speed;\r\n        }\r\n        else {\r\n            this.view.x -= this.speed;\r\n            if (this.view.x < 0) {\r\n                this.onDisable();\r\n            }\r\n        }\r\n        if (this.dis > Laya.Browser.clientWidth) {\r\n            this.onDisable();\r\n        }\r\n    }\r\n    onTriggerEnter(other, self, contact) {\r\n        console.log(\"碰到什么东西了--\", other.label);\r\n    }\r\n    onDisable() {\r\n        this.dis = 0;\r\n    }\r\n}\r\n",
